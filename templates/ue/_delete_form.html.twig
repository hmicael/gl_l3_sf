<form method="post" action="{{ path('app_ue_delete', {'id': ue.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ ue.id) }}">
    <button class="btn btn-outline-danger">Delete <i class="fa-solid fa-trash"></i></button>
</form>
